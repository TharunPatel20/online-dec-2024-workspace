package stringdemo;

public class StringReverseDemo {

	public String reverse1(String str) {
		String result = "";
		for(int i=str.length()-1;i>=0;i--) {
			result += str.charAt(i);
		}
		return result;
	}
	
	public static void main(String[] args) {
		String str = "welcome";
		String output;
		System.out.println(str);
		System.out.println(output);

	}

}
